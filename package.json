{
  "name": "flux",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:web": "npm run generate:icons && cross-env NODE_ENV=development BUILD_PLATFORM=web vite",
    "build:web": "npm run sync:version && cross-env NODE_ENV=production BUILD_PLATFORM=web vite build",
    "preview:web": "cross-env NODE_ENV=production BUILD_PLATFORM=web vite preview",
    "sync:version": "node --loader ts-node/esm scripts/sync-version.ts",
    "generate:icons": "node --loader ts-node/esm scripts/generate-icons.ts"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^2.0.2",
    "jsqr": "^1.4.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "recharts": "^3.5.1",
    "vconsole": "^3.15.1"
  },
  "devDependencies": {
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
